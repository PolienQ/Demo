<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">

    <TextView
        android:id="@+id/https_tv_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="连接状态"/>
    <Button
        android:id="@+id/https_btn_http"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="测试http"/>

    <Button
        android:id="@+id/https_btn_https"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="测试https"/>

    <Button
        android:id="@+id/https_btn_get"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="get请求"/>


    <TextView
        android:id="@+id/https_tv_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

</LinearLayout>